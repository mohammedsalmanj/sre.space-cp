{
    "version": 2,
    "name": "sre-space-dashboard",
    "builds": [
        {
            "src": "apps/dashboard/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/apps/dashboard/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/apps/dashboard/$1"
        }
    ],
    "cleanUrls": true
}