{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/",
            "destination": "/apps/dashboard/index.html"
        },
        {
            "source": "/(style\\.css|main\\.js)",
            "destination": "/apps/dashboard/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/apps/dashboard/$1"
        }
    ]
}