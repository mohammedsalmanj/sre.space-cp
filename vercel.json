{
    "version": 2,
    "name": "sre-space-cp",
    "framework": "other",
    "public": true,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://sre-space-api.vercel.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/apps/frontend/$1"
        }
    ],
    "cleanUrls": true
}